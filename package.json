{
  "name": "bs-puppeteer",
  "version": "0.10.0",
  "description": "Bucklescript bindings for Puppeteer (headless chrome browser) written in Reason",
  "keywords": [
    "BuckleScript",
    "Puppeteer",
    "headless",
    "browser",
    "scraping"
  ],
  "license": "MIT",
  "homepage": "https://github.com/bs-puppeteer/bs-puppeteer",
  "bugs": "https://github.com/bs-puppeteer/bs-puppeteer/issues",
  "repository": "github:bs-puppeteer/bs-puppeteer",
  "author": "Zach Ploskey <zach@ploskey.com>",
  "main": "src/Puppeteer.re",
  "scripts": {
    "build": "bsb -make-world",
    "clean": "bsb -clean-world",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged && bsb -clean-world -make-world && jest",
    "start": "bsb -make-world -w",
    "test": "bsb -make-world && jest",
    "test:ci": "jest --bail --ci",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "puppeteer": "^1.8.0"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.2",
    "bs-platform": "^4.0.2",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2"
  }
}
